(decl fib (n int) int (
	(if (<= n 0) int 0)
	(if (== n 1) int 1
		(+ n (fib (- n 1))))))

(decl fact (n int) int (
	(if (<= n 0) int 0)
	(if (== n 1) int 1
		(* n (fact (- n 1))))))

