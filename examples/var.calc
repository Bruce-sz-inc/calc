; define a as the value 42 in a very round-about way
(define a (var (b:int):int (= b (if true:int 42)) b))

; define c as a function which adds b (zero value) and doubling a, returning
; the result
(define c (func :int (var (b:int):int (+ a a b))))

; call c; result should be 84
(define main (func:int (c)))
